{
    "name": "easilydo/sift-php-sdk",
    "description": "A PHP wrapper around Sift API",
    "type": "library",
    "authors": [
        {
            "name": "Easilydo",
            "email": "production@easilydo.com",
            "homepage": "https://sift.easilydo.com",
